{
  "version": 2,
  "builds": [
    { "src": "/index.js", "use": "@vercel/node" },
    { "src": "/client/build/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/$1"
    }
  ]
}
